function tad(li,con,on){
		li.each(function(j){
			$(this).click(function(){
			$(this).addClass(on).siblings().removeClass(on);	
			con.eq(j).show().siblings().hide();;					
				});
			});
	};	
	

function GWC(){
	var dz_li = $("#gwcJS_DzList ul li");
		dz_li.click(function(){
		$(this).addClass("on").siblings().removeClass("on");	
		$(this).find(".gwcJS_DzLi_BJbutton").show();
		$(this).siblings().find(".gwcJS_DzLi_BJbutton").hide();
		});
	
	var fs_li = $("#gwcJS_ZFlist dl");
		fs_li.each(function(i){
			var rad = $(this).find("input[name^='ZFFS']");
			rad.click(function(){
				if(this.checked){
				$(this).parent().parent().addClass("cur");
				fs_li.eq(i).find("dd").show();
				fs_li.eq(i).siblings().find("dd").hide();
				};
			});		
	});
	
	//var zx_li = $("#gwcJS_ZFli_ZX ul li");
		//zx_li.click(function(){
			//$(this).addClass("on").siblings().removeClass("on");
			//});	
			
	var gwcWZF_li = $("#gwcWZF_pop_con ul li");
		gwcWZF_li.click(function(){
			$(this).addClass("on").siblings().removeClass("on");
			});			
		
	};	
	


