.rich_media{
	font-size: 16px;
    word-wrap: break-word;
    -webkit-hyphens: auto;
    -ms-hyphens: auto;
		hyphens: auto;
}
.rich_media_area_primary{
	position: relative;
    padding: 20px 15px 15px;
    background-color: #fff;
		overflow: hidden;
