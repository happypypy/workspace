@charset "utf-8";

/* CSS Document */
html{
    -ms-text-size-adjust:100%;
    -webkit-text-size-adjust:100%;
    font-size:100px;
}
body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{
    padding:0;
    margin:0;
    border:0;
    vertical-align:baseline;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
h1,h2,h3,h4,h5,h6{
    font-weight:normal;
}
body{
    min-width: 320px;
    font-size: 16px;
    text-rendering: optimizeLegibility;
		font-family: -apple-system-font,"Helvetica Neue","PingFang SC","Hiragino Sans GB","Microsoft YaHei",sans-serif;
		overflow-x: hidden;
}
table{
    margin:auto;
    text-align:left;
    border-collapse:collapse;
    border-spacing:0;
}
ol,ul{
    list-style:none;
    list-style-type:none;
}
a img,img{
    vertical-align:middle;
    border:none;
}

/* a style */
a{
    text-decoration: none;
    -webkit-tap-highlight-color: rgba(0,0,0,0); 
}
a:visited{
    text-decoration: none;
    outline: none;
}
a:hover{
    text-decoration: none;
    outline: none;
}
a:active{
    outline: none;
}
a:focus{
    outline: none;
}

/* form style */
input,textarea,select,button{
    vertical-align: middle;
    outline: none;
    background: inherit;
    appearance: none;
    -webkit-appearance: none;  
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    padding: 0;
		-webkit-tap-highlight-color: rgba(0,0,0,0);
		border: none;
}
label{
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}

/* clearfix style */
.clear{
    clear:both;
    height:0;
    visibility:hidden;
    overflow:hidden;
}
.clearfix:after{
    display: block;
    clear: both;
    height: 0;
    content: "\20";
    visibility: hidden;
    overflow: hidden;
}
.clearfix{
    display: block;
    *zoom: 1;
}
.ovh{
    overflow: hidden;
}
*html .clearfix{
    height: 1%;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.flex{
    display: -webkit-flex;
    display: -webkit-box;
    display: -moz-flex;
    display: -ms-flex;
    display: -o-flex;
    display: flex;
}

.flex-middle{
    -webkit-box-pack: center;
    -moz-box-pack: center;
    -ms-flex-align:center;/* IE 10 */
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
}
.flex-center{
    -webkit-box-align: center;
    -moz-box-align: center;
    -ms-flex-pack:center;  
    -webkit-justify-content: center;
    -moz-justify-content: center;
    justify-content: center;
}
.fx1{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -moz-flex: 1;
    -ms-flex: 1;
    -o-flex: 1;
    flex: 1;
}
.space-between{
    -webkit-justify-content: space-between;
    justify-content: space-between;
}

@font-face {font-family: 'iconfont';
    src: url('iconfont.eot'); /* IE9*/
    src: url('iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('iconfont.woff') format('woff'), /* chrome?firefox */
    url('iconfont.ttf') format('truetype'), /* chrome?firefox?opera?Safari, Android, iOS 4.2+*/
    url('iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont{
    font-family:"iconfont" !important;
    font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.001rem;
    -moz-osx-font-smoothing: grayscale;
}


body,
html{
	height: 100%;
}

/*page1*/
.site-section{
	position:relative;
	min-width: 320px;
	max-width: 640px;
	margin: 0 auto;
	font-size: 0.16rem;
	padding-top: 0.5rem;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	min-height: 100%;
}
.index-bg{
	/* background: url("../../images/bg_03.jpg") no-repeat center top; */
	background-size: 3.2rem 9.3rem;
	background-attachment: fixed;
}
header{
	height: 0.5rem;
	background: rgba(255,255,255,.95);
	padding: 0 0.1rem;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	z-index: 9;
	box-shadow: 0 0 6px rgba(0,0,0,.2);
}
header .logo{
	height: 0.32rem;
	margin-top: 0.09rem;
	display: block;
	float: left;
}
header .logo img{
	height: 0.32rem;
	width: auto;
}
header .btns{
	float: right;
	margin-top: 0.135rem;
}
header .btns a{
	display: block;
	float: left;
	width: 0.265rem;
	height: 0.215rem;
	background-size: 0.265rem 0.215rem;
	background-repeat: no-repeat;
	background-position: 0;
	margin-left: 0.05rem;
}
header .btns .btn1 {
  background-image: url('../../images/header_btn1_03.png');
}
header .btns .btn2 {
  background-image: url('../../images/header_btn2_05.png');
}
header .btns .btn3 {
  background-image: url('../../images/header_btn3_03.jpg');
  display: none;
}


.banner{
	padding: 0 0.09rem;
	position: relative;
	/* margin-bottom: .1rem; */
}
.banner .swiper-container{
	border-radius: 0.03rem;
	box-shadow: 0 0 0.05rem rgba(0, 0, 0, .34);
}
.banner img{
	width: 100%;
	border-radius: 0.03rem;
}
.banner .swiper-pagination{
	bottom: 0.03rem;
	width: 100%;
	left: 0;
	z-index:  2;
}
.banner .swiper-pagination-bullet{
	width: 0.06rem;
	height: 0.06rem;
	background: #fff;
	opacity: .8;
	margin: 0 0.025rem;
}
.banner .swiper-pagination-bullet-active{
	background: #fff;
	opacity: 1;
}

.nav{
	/* height: 1.77rem; */
	padding-top: 0.19rem;
	display: flex;
	flex-wrap:wrap;
}
.nav li{
	flex:0 0 25%;
	overflow: hidden;
	text-align: center;
	margin-bottom:0.1rem;
}
.nav li img{
	height: 0.35rem;
}
.nav li p{
	font-size: 0.12rem;
	line-height: 1;
	color: #666;
	padding-top: 0.05rem;
}

.bar{
	height: 0.75rem;
}
.bar a{
	display: block;
	width: 1.45rem;
	height: 0.75rem;
	float: left;
	margin-left: 0.1rem;
}
.bar img{
	width: 1.45rem;
	display: block;
	border-radius: 0.03rem;
}

.list{
  padding: 0.21rem 0.09rem 0.1rem;
  overflow: hidden;
  display: flex;
  flex-wrap: wrap;
}
.list li{
  margin-bottom: 0.1rem;
  width: 100%;
  border-radius: 0.03rem;
  box-shadow: 0 0.01rem 0.05rem rgba(0, 0, 0, 0.24);
}
.list img{
  width: 100%;
  border-top-left-radius: 0.03rem;
  border-top-right-radius: 0.03rem;
}
.list .word{
  border-bottom-left-radius: 0.03rem;
  border-bottom-right-radius: 0.03rem;
  background: #fff;
  position: relative;
  padding: 0 0.07rem 0.08rem;
}
.common-list .noword{
	width: 100%;
	text-align: center;
	color: #999;
	font-size: 14px;
}
.list .word .tit{
  font-size: 0.14rem;
  color: #333;
  line-height: 1.5;
  padding: 0.05rem 0 0.03rem;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.list .word .txt{
  font-size: 0.12rem;
  color: #999;
  line-height: 0.14rem;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.list .word .info{
  font-size: 0.09rem;
  color: #fff;
  line-height: 0.22rem;
  height: 0.22rem;
  position: absolute;
  width: 100%;
  left: 0px;
  top: -0.22rem;
  background: rgba(0, 0, 0, 0.5);

}
.list .word .view{
	float: left;
	padding-left: 0.07rem;
}
.list .word .time{
	float: right;
	padding-right: 0.07rem;
}


.menu{
	position: fixed;
	top: 0;
	right: 0;
	width: 78%;
	height: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	padding-top: 0.3rem;
	background: #373a41;
	z-index: 99;
	-webkit-transform: translateX(100%);
	-ms-transform: translateX(100%);
	-o-transform: translateX(100%);
	transform: translateX(100%);
}
header,
.site-section,
.menu{
	-webkit-transition: transform 0.3s;
	-o-transition: transform 0.3s;
	transition: transform 0.3s;
}
.menu-cover{
	opacity: 0;
	background: #fff;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 98;
	position: fixed;
	display: none;
}
body.open-menu .menu{
	-webkit-transform: translateX(0);
	-ms-transform: translateX(0);
	-o-transform: translateX(0);
	transform: translateX(0);
	overflow-y: scroll;
}
body.open-menu header,
body.open-menu .site-section{
	-webkit-transform: translateX(-78%);
	-ms-transform: translateX(-78%);
	-o-transform: translateX(-78%);
	transform: translateX(-78%);
}
body.open-menu header .btns .btn2{
	display: none;
}
body.open-menu header .btns .btn3{
	display: block;
}
body.open-menu .menu-cover{
	display: block;
}

.menu ul li a{
	font-size: 0.13rem;
	color: #fff;
	line-height: 0.42rem;
	display: block;
	padding-left: 0.35rem;
}
.menu ul li{
	position: relative;
	border-bottom: 1px solid #e0e0e0;
}
.menu ul li:after{
	background: rgba(255,255,255,.2);
}


.information-list ul{
	background: #fff;
	padding: 3px 0 4px;
}
.information-list ul li{
	display: -webkit-flex;
	display: -moz-flex;
	display: -ms-flex;
	display: -o-flex;
	display: flex;
	position: relative;
	padding: 8px 0;
	font-size: 15px;
}
.information-list ul li:not(:last-child):after{
	content: '';
	display: block;
	height: 1px;
	width: 100%;
	position: absolute;
	bottom: 0;
	left: 0;
	background: #ccc;
	-webkit-transform-origin: 0 100%;
	transform-origin: 0 100%;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5)
}
.information-list ul li .tit{
	color: #777;
	line-height: 25px;
	text-align: right;
	width: 8em;
}
.information-list ul li .txt{
	-webkit-flex: 1;
	-moz-flex: 1;
	-ms-flex: 1;
	-o-flex: 1;
	flex: 1;
	padding: 0 5px;
}
.information-list ul li .txt input{
	border: none;
	background: none;
	height: 24px;
	line-height: 24px;
	font-size: 16px;
	width: 100%;
	display: block;
	color: #333;
}
.information-btn{
	margin-top: 20px;
	text-align: center;
}
.information-btn input{
	font-size: 16px;
	padding: 6px 24px;
	border-radius: 4px;
	border: 0;
}
.information-btn input[type=button]{
	background: #fff;
	color: #777;
	margin-right: 10px;
}
.information-btn input[type=submit]{
	background: #ff7800;
	color: #fff;
}
.information-list ul li .txt .sel{
	position: relative;
	height: 24px;
	display: -webkit-box;
}
.information-list ul li .txt .sel select{
	width: 100%;
	border: none;
	background: transparent;
	height: 24px;
	font-size: 15px;
	color:#333;
	display: block;
	-webkit-box-flex: 1;
	-webkit-appearance: none;
}
.information-list ul li .txt .sel span.spsel{
	display: block;
	position: relative;
	width: 25px;
	height: 24px;
}
.information-list ul li .txt .sel span.spsel:before{
	content: '';
	height: 10px;
	width: 10px;
	display: block;
	border: 2px solid #888;
	border-left-width: 0;
	border-top-width: 0;
	transform: rotate(-45deg);
	-webkit-transform: rotate(-45deg);
	-moz-transform: rotate(-45deg);
	-o-transform: rotate(-45deg);
	-ms-transform: rotate(-45deg);
	position: absolute;
	top: 7px;
	right: 7px;
}
.information-list ul li .txt .picfile{
	position: relative;
	width: 65px;
	height: 65px;
	border: 1px solid #ccc;
	float: left;
}
.information-list ul li .txt .picfile:before, .information-list ul li .txt .picfile:after {
	content: '';
	height: 1px;
	width: 33px;
	display: block;
	background: #ccc;
	position: absolute;
	top: 32px;
	left: 16px;
}
.information-list ul li .txt .picfile:after {
	height: 33px;
	width: 1px;
	top: 16px;
	left: 32px;
}
.information-list ul li .head{
	font-size: 16px;
	font-weight: bold;
	padding-left: 1em;
}
.information-list ul li .info{
	line-height: 25px;
}
.information-list ul li .info .name{
	color: #0000e4;
}
.information-list ul li .info .blue{
	color: #81abe5;
}


.pw{
	font-size: 14px;
	color: #5a5a5a;
	padding: 7px 0;
	text-align: left;
	text-indent: 5px;
	border-radius: 4px;
	border: 1px solid #cacaca;
	margin-bottom: 5px;
	font-size: 14px;
}
.pwon{
	font-size: 14px;
	color: #5a5a5a;
	padding: 7px 0;
	text-align: left;
	text-indent: 5px;
	border-radius: 4px;
	border: 2px solid #ff7902;
	margin-bottom: 4px;
	color: black;
}
.pw span,
.pwon span{
	color: red;
}
.pay-txt{
	font-size: 16px;
	color: #3b3b3b;
	line-height: 2.2;
	position: relative;
	text-indent: 12px;
	width: 100%;
	box-sizing: border-box;
}
.pay-txt:after{
	content: '';
	display: block;
	height: 1px;
	width: 100%;
	bottom: 0;
	left: 0;
	background: #e3e3e3;
	-webkit-transform-origin: 0 100%;
	transform-origin: 0 100%;
	-webkit-transform: scaleY(0.5);
	transform: scaleY(0.5)
}
.pay-style{
	text-align: center;
	padding: 20px 0 0;
}
.pay-style img{
	width: auto;
	height: 64px;
}
.pay-style p{
	font-size: 16px;
	color: #3b3b3b;
	margin-top: 5px;
	line-height: 1.5;
}
.pay-style .num{
	font-size: 16px;
	color: #3b3b3b;
	line-height: 1.5;
}
.pay-style .price{
	font-size: 36px;
	line-height: 1.5;
	margin-top: 5px;
}
.pay-style a.pay{
	display: block;
	width: 290px;
	margin: 10px auto 0;
	background: #1aad16;
	border-radius: 3px;
	height: 40px;
	line-height: 40px;
	color: #fff;
	font-size: 15px;
}
.choose-num{
	background: #fff;
	overflow: hidden;
	font-size: 13px;
	color: #555;
	width: 100%;
	padding: 5px 0;
}
.choose-num>div{
	float: left;
}
.choose-num .tit{
	line-height: 27px;
	padding-left: 15px;
}
.choose-num .most{
	line-height: 27px;
	padding-left: 6px;
}
.choose-num .minus{
	float: left;
	border: 1px solid #e3e3e3;
	width: 22px;
	height: 25px;
	position: relative;
	background: -webkit-linear-gradient(#fff, #f7f7f7);
	border-right: none;
}
.choose-num .plus{
	float: left;
	border: 1px solid #e3e3e3;
	width: 22px;
	height: 25px;
	position: relative;
	background: -webkit-linear-gradient(#fff, #f7f7f7);
	border-left: none;
}
.choose-num .minus:after{
	content: '';
	display: block;
	position: absolute;
	top: 11px;
	left: 5px;
	width: 12px;
	height: 2px;
	background: #959393;
}
.choose-num .plus:after{
	content: '';
	display: block;
	position: absolute;
	top: 11px;
	left: 7px;
	width: 8px;
	height: 2px;
	background: #959393;
}
.choose-num .plus:before{
	content: '';
	display: block;
	position: absolute;
	top: 7px;
	left: 10px;
	width: 2px;
	height: 10px;
	background: #959393;
}
.choose-num .num{
	float: left;
	border: 1px solid #d8d7d7;
	width: 33px;
	height: 25px;
	position: relative;
	text-align: center;
	background: -webkit-linear-gradient(#f7f7f7,#fff);
	line-height: 25px;
}
.price-box{
	padding: 10px;
	font-size: 14px;
	text-align: right;
	line-height: 22px;
}
.price-box dd{
	vertical-align: middle;
	padding: 3px 0;
}
.price-box .red{
	color: red;
}
.price-txt{
	border: none;
	border-bottom: 1px solid #ddd;
	width: 2em;
	text-align: center;
	color: red;
}
.price-box .submit{
	background: #ff7800;
	color: #fff;
	border: none;
	outline: none;
	padding: 4px 12px;
	border-radius: 3px;
}

.price-box input[type='checkbox'] {
	display: none;
	cursor: pointer;
}
.price-box input[type='checkbox']:focus,
.price-box input[type='checkbox']:active {
	outline: none;
}
.price-box input[type='checkbox'] + label {
	cursor: pointer;
	display: inline-block;
	position: relative;
	padding-right: 25px;
}
.price-box input[type='checkbox'] + label:before,
.price-box input[type='checkbox'] + label:after {
	content: '';
	font-family: helvetica;
	display: inline-block;
	width: 18px;
	height: 18px;
	right: 0;
	bottom: 2px;
	text-align: center;
	position: absolute;
}
.price-box input[type='checkbox'] + label:before {
	background-color: #f5f5f5;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-webkit-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.price-box input[type='checkbox'] + label:after {
	content: "\2713";
	line-height: 18px;
	font-size: 14px;
	color: #fff;
}
.price-box input[type='checkbox']:checked + label:before {
	-moz-box-shadow: inset 0 0 0 10px #158EC6;
	-webkit-box-shadow: inset 0 0 0 10px #F44336;
	box-shadow: inset 0 0 0 10px #F44336;
	border: 1px solid #;
}

.price-box input[type='checkbox'] + label:before {
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}
.price-box input[type='checkbox']:checked + label:after {
	content: "\2713";
	line-height: 18px;
	font-size: 14px;
	color: #fff;
}


.order-confirm{
	position: fixed;
	z-index: 999;
	width: 90%;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%,-50%);
	-ms-transform: translate(-50%,-50%);
	-o-transform: translate(-50%,-50%);
	transform: translate(-50%,-50%);
	background: #fff;
	border-radius: 4px;
	overflow: hidden;
	display: none;
}
.order-confirm .head{
	background: #f8f8f8;
	padding: 12px 20px;
	font-size: 16px;
	overflow: hidden;
}
.order-confirm .head p{
	color: #333;
}
.order-confirm .txt{
	padding: 10px 40px 14px;
	font-size: 16px;
}
.order-confirm .txt .item{
	padding: 3px 0;
}
.order-confirm .txt .tit{
	color: #666;
}
.order-confirm .txt .info{
	color: #000;
}
.order-confirm .btn{
	margin-top: 10px;
	justify-content: space-between;
}
.order-confirm .btn a{
	display: block;
	margin-right: 15px;
	/*padding: 0 12px;*/
	width: 40%;
	text-align: center;
	font-size: 16px;
	line-height: 32px;
	border-radius: 3px;
}
.order-confirm .btn .continue{
	background: #4b8be6;
	color: #fff;
}
.order-confirm .btn .return{
	background: #f0f0f0;
	color: #333;
}
.close-confirm{
	display: block;
	background: url(../../images/x.png) no-repeat center;
	width: 16px;
	height: 16px;
	background-size: cover;
}
.gray-cover{
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 998;
	background: rgba(0,0,0,.6);
}



.comment-main{
	background: #fff;
	margin-top: 10px;
	padding: 10px 10px 20px;
	margin-bottom: 10px;
	box-shadow: 0 0 6px rgba(0,0,0,.2);
}
.comment-main-tit{
	font-size: 14px;
	color: #fa923a;
}
.comment-head{
	width: 30px;
	height: 30px;
	display: block;
	border-radius: 100%;
	margin-right: 5px;
}
.comment-item{
	padding: 10px 0;
	position: relative;
}
.comment-item:after{
	content: '';
	display: block;
	position: absolute;
	bottom: 0;
	left: 0;
	width: 100%;
	height: 1px;
	background: #ddd;
	-webkit-transform: scaleY(0.5);
	-ms-transform: scaleY(0.5);
	-o-transform: scaleY(0.5);
	transform: scaleY(0.5);
	-webkit-transform-origin: 0 0;
	-ms-transform-origin: 0 0;
	-o-transform-origin: 0 0;
	transform-origin: 0 0;
	z-index: 1;
}
.comment-item .name{
	line-height: 22px;
	height: 22px;
	color: #808080;
}
.comment-item .name p{
	float: left;
	font-size: 14px;
}
.comment-item .name span{
	float: right;
	font-size: 13px;
}
.comment-item .txt{
	word-wrap: break-word;
	white-space: normal;
	word-break: break-all;
	font-size: 14px;
	color: #000;
	padding: 2px 0 0;
}
.comment-item .author{
	margin-top: 4px;
	margin-bottom: 6px;
	margin-left: 10px;
}
.comment-item .author-tit{
	position: relative;
	color: #999;
	font-size: 14px;
	line-height: 20px;
	padding-left: 7px;
	margin-bottom: 2px;
}
.comment-item .author-txt{
	padding-left: 7px;
	font-size: 14px;
	color: #333;
}
.comment-item .author-tit:before{
	content: '';
	display: block;
	position: absolute;
	width: 3px;
	height: 13px;
	background: #4CAF50;
	top: 3px;
	left: 0;
}
.comment-item .info{
	color: #808080;
	font-size: 14px;
	overflow: hidden;
	line-height: 1.6;
}
.comment-item .info .reply{
	padding-left: 18px;
	position: relative;
}
.comment-item .info .reply:before{
	content: '\e63b';
	display: block;
	font-family: 'iconfont';
	position: absolute;
	left: 0;
	top: 2px;
	line-height: 18px;
	font-size: 14px;
	color: #808080;
}
.comment-item .info .zan{
	padding-left: 18px;
	position: relative;
	margin-right: 10px;
}
.comment-item .info .zan:before{
	content: '\e871';
	display: block;
	font-family: 'iconfont';
	position: absolute;
	left: 0;
	top: 0;
	line-height: 18px;
	font-size: 14px;
	color: #808080;
}
.comment-item .info .zan.on:before{
	content: '\e870';
}
.comment-main>div:last-of-type:after{
	display: none;
}
.detail-more{
	color: #fff;
	background: #fa923a;
	text-align: center;
	border-radius: 4px;
	font-size: 14px;
	padding: 10px 0;
	line-height: 1.2;
	display: block;
	margin-top: 5px;
}
.android .detail-more{
	padding-bottom: 9px;
}

.comment-edit{
	padding: 10px;
	background: #fff;
}
.comment-edit .tit{
	color: #fa923a;
	font-size: 15px;
}

.comment-edit textarea{
	background: #eee;
	font-size: 13px;
	line-height: 1.5;
	padding: 4px 8px;
	display: block;
	width: 100%;
	height: 80px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	resize: none;
	border-radius: 3px;
	border-color: #d2d2d2;
	margin-top: 10px;
}
.comment-edit .submit{
	background: #fa923a;
	display: inline-block;
	border: none;
	outline: none;
	color: #fff;
	font-size: 13px;
	padding: 6px 12px;
	border-radius: 3px;
	margin-top: 10px;
}

.delete,
.good{
	background: #fa923a;
	opacity: 0.84;
	width: 36px;
	height: 36px;
	border-radius: 100%;
	box-shadow: 0 0 4px rgba(0,0,0,.4);
	color: #fff;
	line-height: 36px;
	text-align: center;
	position: fixed;
	top: 40%;
	right: 15px;
	font-size: 13px;
	z-index: 99;
}
.good{
	top: calc(40% + 50px);
}

