	height: 420px;
}
.select-main.h336.active{
	height: 336px;
}
.select-main.h294.active{
	height: 600px;
}
.select-main.h252.active{
	height: 252px;
}
.select-main.h126.active{
	height: 126px;
}
.select-main.h84.active{
	height: 84px;
}
.select-main.h42.active{
	height: 42px;
}
.select-main>div{
	-webkit-box-flex: 1;
	box-sizing: border-box;
	width: 50%;
	position: relative;
}
.select-main>div{
	overflow: hidden;
}
.select-main li{
	line-height: 42px;
	height: 42px;
	padding-left: 28px;
