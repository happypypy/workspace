.site-section{
	margin-left: auto;
	margin-right: auto;
	padding-bottom: 10px;
	width: 100%;
}
.select-bar{
	width: 100%;
	background: #fff;
	z-index: 9;
}
.select-bar.fixed{
	position: fixed;
	top: 0;
	left: 0;
}
.select-bar-tab{
	display: -webkit-box;
	border-bottom: 1px solid #ddd;
}
.select-bar-tab li{
	-webkit-box-flex: 1;
	text-align: center;
	position: relative;
	font-size: 15px;
	color: #666;
	box-sizing: border-box;
	height: 36px;
	line-height: 32px;
	width: 50%;
	border-right: 1px solid #ddd;
}
.select-bar-tab li.on,
.select-bar-tab li.on span{
	color: #ff7902;
}
.select-bar-tab li.btn:after{
	content: "";
	width: 0;
	height: 0;
	display: inline-block;
	border-top: 8px solid #B7B7B7;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-bottom: 0;
	margin-left: 4px;
	/* vertical-align: -1px; */
}
.select-bar-tab li:last-child{
	border: none;
}
.select-bar-tab li span,
.select-bar-tab li a{
	display: inline-block;
	white-space: nowrap;
	overflow: hidden;
	max-width: 80%;
	vertical-align: middle;
	text-overflow: ellipsis;
	color: #666;
}
.select-bar-tab li:not(:last-child).on:after{
	border-top: 0;
	border-left: 4px solid transparent;
	border-right: 4px solid transparent;
	border-bottom: 8px solid #ff7902;
	vertical-align: 1px;
}
.select-bar-tab li i.search{
	vertical-align: -1px;
	margin-left: 2px;
}
.section-shade{
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	background: rgba(0,0,0,.7);
	display: none;
	z-index: 8;
}
.select-wrapper{
	position: relative;
}
.select-main{
	width: 100%;
	display: -webkit-box;
	background: #f6f4f0;
	height: 0;
	overflow: hidden;
	-webkit-transition: height .3s;
	position: absolute;
}
