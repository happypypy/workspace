      content: '\e60d';
      font-family: 'iconfont';
      margin-right: 3px;
    }
    &.state::before {
      display: inline-block;
      content: '\e60e';
      font-family: 'iconfont';
      margin-right: 3px;
    }
    &.node::before {
      display: inline-block;
      content: '\e611';
      font-family: 'iconfont';
      margin-right: 3px;
    }
