/* 活动图文 */
.activity-wrap {
  background: #fff;
  margin-bottom: 0.1rem;
  padding: 0.1rem;
}
.activity-list {
  flex-wrap: wrap;
  .no-data {
    flex: 0 0 100%;
    font-size: 0.14rem;
    color: #999;
    line-height: 0.32rem;
    padding: 0 0 0 0.1rem;
    border: 1px solid #eee;
    margin-bottom: 0;
    border-radius: 4px;
  }
}
.activity-item-s {
  font-size: 0.14rem;
  color: #999;
  padding: 0.1rem 0;
  border-bottom: 1px solid #eee;
  .border-radius();
  &:first-child {
    padding-top: 0;
  }
  &:last-child {
    padding-bottom: 0;
    border-bottom: none;
  }
  .activity-item-img {
    flex: 0 0 0.8rem;
    height: 0.8rem;
    margin-right: 0.06rem;
    .border-radius(7px);
    overflow: hidden;
    position: relative;
    > img {
      width: 100%;
      height: 100%;
    }
  }
  .activity-item-txt {
    font-size: 0.12rem;
    flex: 1;
    color: #666;
    line-height: 0.21rem;
    padding-top: 0.02rem;
  }
  .activity-item-title {
    justify-content: space-between;
    align-items: center;
    line-height: 0.18rem;
    > .title {
      color: #333;
      font-size: 0.15rem;
      font-weight: 900;
      .txt-cut(1);
    }
    > .iconfont {
      font-size: 0.16rem;
      font-weight: normal;
    }
  }
  .activity-item-tag {
    width: fit-content;
    padding: 0 0.08rem;
    line-height: normal;
    margin-top: 0.03rem;
    color: #fff;
    .border-radius();
    font-size: 0.12rem;
    align-items: center;
    justify-content: flex-start;
    .tag,
    .age {
      max-width: 4em;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }
  }
}

.activity-item-m {
  flex: 0 0 calc((3.55 - 0.06) / 2rem);
  width: calc((3.55 - 0.06) / 2rem);
  font-size: 0.14rem;
  color: #999;
  border: 1px solid #eee;
  margin-right: 0;
  margin-top: 0.06rem;
  .border-radius(6px);
  position: relative;
  &:nth-child(2n + 1) {
    margin-right: 0.06rem;
  }
  &:nth-child(-n + 2) {
    margin-top: 0;
  }
  &.no-data {
    margin-right: 0;
  }
  .activity-item-img {
    flex: 0 0 100%;
    height: 0.86rem;
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
    overflow: hidden;
    position: relative;
    > img {
      width: 100%;
      height: 100%;
    }
  }
  .activity-item-txt {
    font-size: 0.12rem;
    flex: 1;
    color: #666;
    line-height: 0.21rem;
    padding: 0.08rem 0.05rem;
  }
  .activity-item-title {
    justify-content: space-between;
    align-items: baseline;
    line-height: 0.18rem;
    > .title {
      color: #333;
      font-size: 0.15rem;
      font-weight: 900;
      .txt-cut(2);
    }
    > .iconfont {
      font-size: 0.16rem;
      font-weight: normal;
    }
  }
  .activity-item-tag {
    color: #999;
    font-size: 0.12rem;
    line-height: normal;
    align-items: center;
    justify-content: space-between;
    .tag,
    .age {
      padding: 0 0.015rem;
      .border-radius();
      border: 1px solid #eee;
      max-width: 4.5em;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }
  }
}

.activity-addr {
  position: absolute;
  bottom: 0;
  left: 0;
  background: #494949;
  color: #fff;
  border-top-right-radius: 7px;
  font-size: 0.12rem;
  padding: 0 0.07rem;
  line-height: 0.15rem;
}
.activity-item-word {
  font-size: 0.12rem;
  line-height: 0.18rem;
  color: #666;
  .txt-cut(1);
  margin-bottom: 0.05rem;
  padding-right: 0.1rem;
  flex: auto;
}
.activity-price {
  color: #666;
  font-size: 0.12rem;
  margin-right: 0.16rem;
  > span {
    font-weight: bold;
    font-size: 0.12rem;
    > em {
      font-weight: bold;
      font-size: 0.16rem;
    }
  }
}
.activity-item-view {
  display: flex;
  justify-content: flex-start;
  line-height: 0.18rem;
  .eyes,
  .time {
    font-size: 0.12rem;
    color: #999;
  }
  .eyes {
    margin-right: 0.07rem;
    &::before {
      display: inline-block;
      content: '\e60b';
      font-family: 'iconfont';
    }
  }
  .time {
    &::before {
      display: inline-block;
      content: '\e606';
      font-family: 'iconfont';
      margin-right: 1px;
    }
  }
}

.activity-item-b {
  flex: 0 0 100%;
  width: 100%;
  font-size: 0.14rem;
  color: #999;
  border: 1px solid #eee;
  margin-bottom: 0.06rem;
  .border-radius(10px);
  position: relative;
  &:last-child {
    margin-bottom: 0;
  }
  .activity-item-img {
    flex: 0 0 100%;
    height: 1.77rem;
    border-top-left-radius: 10px;
    border-top-right-radius: 10px;
    overflow: hidden;
    position: relative;
    > img {
      width: 100%;
      height: 100%;
    }
  }
  .activity-item-txt {
    padding: 0.08rem 0.06rem;
    font-size: 0.12rem;
    flex: 1;
    color: #666;
    line-height: 0.21rem;
  }
  .activity-item-title {
    justify-content: space-between;
    align-items: baseline;
    line-height: 0.18rem;
    > .title {
      color: #333;
      font-size: 0.15rem;
      font-weight: 900;
      .txt-cut(1);
    }
    > .iconfont {
      font-size: 0.16rem;
      font-weight: normal;
    }
  }
  .activity-item-view {
    justify-content: center;
    align-items: center;
  }
  .node-item-view {
    justify-content: flex-start;
  }
  .activity-price {
    margin: 0 0.1rem;
    em {
      font-size: 0.2rem;
    }
  }
  .activity-view-inner {
    justify-content: space-between;
  }
  .activity-token {
    font-size: 0.16rem;
    align-self: flex-start;
  }
  .activity-item-tag {
    color: #999;
    font-size: 0.12rem;
    line-height: normal;
    align-items: center;
    justify-content: flex-start;
    .tag,
    .age {
      padding: 0 0.015rem;
      .border-radius();
      border: 1px solid #eee;
      max-width: 4.5em;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
      margin-right: 0.03rem;
    }
  }
}

/* 页面标签 */
.headtitle {
  height: 0.42rem;
  background-color: #fff;
  font-size: 0.18rem;
  border-bottom: 1px solid #eee;
  padding: 0 0.1rem;
  line-height: 0.42rem;
  font-weight: bold;
  color: #333;
  text-align: center;
}

/* detail页面 */

.rich-media {
  padding: 0.1rem;
  background: #fff;
  > h3 {
    font-size: 0.15rem;
    color: #333;
    margin-bottom: 0.1rem;
  }
}
.get-coupon {
  background: url('../images/coupon_bg.jpg') no-repeat center;
  background-size: cover;
  padding: 0.1rem;
  margin-bottom: 0.1rem;
}
.get-coupon .tit {
  font-size: 0.14rem;
  font-weight: bold;
  color: #c03563;
  text-align: center;
}
.get-coupon .tit span {
  display: inline-block;
  position: relative;
  padding: 0 0.3rem;
}
.get-coupon .tit span:before,
.get-coupon .tit span:after {
  content: '';
  display: block;
  position: absolute;
  top: 50%;
  left: 0;
  width: 0.15rem;
  height: 1px;
  background: #c03563;
}
.get-coupon .tit span:after {
  left: auto;
  right: 0;
}
.get-coupon .tips {
  text-align: center;
  font-size: 0.12rem;
  color: #c03563;
  margin-top: 0.05rem;
}
.get-coupon .coupon {
  background: url('../images/coupon_03.png') no-repeat center;
  background-size: contain;
  width: 2.8rem;
  margin: 0.1rem auto 0;
  height: 0.8rem;
  align-items: center;
  padding-right: 0.1rem;
}
.get-coupon .coupon .price {
  font-size: 0.36rem;
  font-weight: bold;
  color: #fff;
  margin-left: 0.32rem;
  width: 0.7rem;
  text-align: center;
}
.get-coupon .coupon .txt {
  color: #fff;
  font-size: 0.14rem;
  width: 0.58rem;
  line-height: 1.4;
  margin-left: 0.08rem;
}
.get-coupon .coupon .draw {
  text-align: center;
  color: #333;
  font-size: 0.14rem;
  font-weight: bold;
  border-radius: 0.17rem;
  line-height: 0.34rem;
  display: block;
  width: 0.86rem;
  height: 0.34rem;
  background: #f8f84f;
  background-size: contain;
  margin-left: 0.17rem;
  box-shadow: 0 3px 0 #000;
}

.activity-info {
  padding: 0.1rem 0;
  border-top: 1px dashed #eee;
  border-bottom: 1px dashed #eee;
  margin-bottom: 0.1rem;
  > ul > li {
    font-size: 0.14rem;
    line-height: 0.21rem;
    span.first {
      .t6;
    }
    span.second {
      .t9;
      &.cost {
        height: 0.21rem;
        padding: 0 0.1rem;
        .border-radius();
      }
    }
  }
}

.act-schedule {
  background-color: #fff;
  word-break: break-all;
}
.act-schedule-in {
  padding: 0 0 0.1rem;
  .border-radius(10px 10px 4px 4px);
  border-left: 1px solid #eee;
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
}
.act-schedule-tit {
  height: 0.5rem;
  background: url(../images/act_schedule_tit.jpg) no-repeat;
  background-size: 100% 100%;
  font-size: 0.21rem;
  line-height: 2;
  font-weight: bold;
  text-align: center;
  color: #fff;
}
.act-schedule-date {
  color: red;
  font-size: 0.12rem;
  text-align: center;
  line-height: 1.5;
}
.act-schedule-item {
  padding: 0.03rem 0.1rem;
}
.act-schedule-time {
  padding-top: 0.03rem;
  font-size: 0.12rem;
  color: red;
  text-align: center;
}
.act-schedule-item .inner {
  border: 1px solid #d2d2d2;
  .border-radius();
  padding: 0 0.1rem;
}
.act-schedule-item .inner .tit {
  color: #333;
  font-size: 14px;
  line-height: 1.5;
}
.act-schedule-item .inner .info {
  justify-content: space-between;
  align-items: center;
  font-size: 0.12rem;
  line-height: 1.5;
}
.act-schedule-item .inner .user-info {
  padding: 0.05rem 0 0.08rem;
}
.info-left-tag {
  .border-radius();
  padding: 0 0.05rem;
  width: fit-content;
  line-height: 1.2;
  font-size: 0.1rem;
}
.info-left-price {
  color: #606060;
  line-height: 0.28rem;
}
.info-left-price em {
  font-size: 0.18rem;
  font-style: normal;
}
.info-left-img {
  align-items: center;
  padding-bottom: 0.04rem;
  color: #333;
}
.info-left-img > img {
  width: 0.24rem;
  height: 0.24rem;
  border-radius: 50%;
}
.act-schedule-item .info .info-right {
  font-size: 0.14rem;
  color: #fff;
  .border-radius();
  padding: 0 0.1rem;
  line-height: 0.28rem;
}
.act-schedule-rule {
  padding-left: 12px;
  color: #666;
  font-size: 12px;
}

.amount-wrap {
  padding: 0 0.1rem;
  font-size: 0.12rem;
  color: #666;
  align-items: center;
  justify-content: space-between;
}
.amount-wrap-img {
  height: 0.24rem;
}
.amount-wrap-img > img {
  width: 0.24rem;
  height: 0.24rem;
  border-radius: 50%;
}

.comment-list {
  flex-wrap: wrap;
}
.comment-list-item {
  padding-bottom: 0.1rem;

  .list-item-img {
    flex: 0 0 0.25rem;
    height: 0.25rem;
    margin-right: 0.05rem;
    border-radius: 50%;
    overflow: hidden;
    > img {
      width: 100%;
      height: 100%;
    }
  }
  .list-item-txt {
    flex: 1;
    .first {
      margin-bottom: 0.1rem;
    }
    .info {
      justify-content: space-between;
      .username {
        font-size: 0.12rem;
        .t6;
        line-height: 1;
      }
      .date {
        font-size: 0.12rem;
        line-height: 0.18rem;
        .t9;
      }
      .floor {
        font-size: 0.12rem;
        color: #999;
      }
    }
    .text {
      .t3;
      .txt-cut(3);
      font-size: 0.14rem;
    }
  }
}
.all-comment {
  .border-radius();
  height: 0.3rem;
  line-height: 0.3rem;
  align-self: flex-end;
  color: #fff;
  font-size: 0.14rem;
  text-align: center;
  width: 100%;
}

.docs-picture {
  overflow: hidden;
  font-size: 0.14rem;
  color: #666;
}
.docs-picture * {
  max-width: 100% !important;
  font-size: 0.14rem;
  padding: 0.1rem;
}
.docs-picture img {
  height: auto !important;
}
/* waiter页面 */

.service-title {
  justify-content: space-between;
  background: #fff;
  padding: 0 0.1rem;
  line-height: 0.4rem;
  border-bottom: 1px solid #eee;
}
.service-title > h3 {
  align-items: center;
  flex: 1;
  border: none;
  padding: 0;
  > img {
    width: 0.32rem;
    margin-right: 0.05rem;
  }
  > span {
    font-size: 0.16rem;
  }
}
.close-service {
  font-size: 0.21rem;
}
.qrcode-wrap {
  flex: 0 0 0.8rem;
  height: 0.8rem;
  > img {
    width: 100%;
    height: 100%;
  }
}
.waiter-info {
  height: 100%;
  justify-content: space-around;
}
.waiter-user {
  position: relative;
  font-size: 0.14rem;
  color: #333;
  &::before {
    display: inline-block;
    font-family: 'iconfont';
    content: '\e620';
    font-size: 0.16rem;
  }
}
.waiter-tel {
  position: relative;
  font-size: 0.14rem;
  &::before {
    display: inline-block;
    font-family: 'iconfont';
    content: '\e60c';
    font-size: 0.16rem;
  }
}

/* reg页面 */
.regImg {
  margin-bottom: 0.1rem;
  > img {
    width: 100%;
    height: auto;
    display: block;
  }
}
.introduction {
  background: #fff;
  padding: 0.1rem;
  > p {
    text-indent: 2em;
    .t6;
    line-height: 1.5;
    font-size: 0.14rem;
  }
}
.reg-form {
  background: #fff;
  padding: 0.1rem;
  .tit {
    font-size: 0.14rem;
    color: #666;
    width: 100%;
    display: block;
  }
  .txt {
    .t6;
    border: 1px solid #ccc;
    height: 0.36rem;
    width: 100%;
    font-size: 0.14rem;
    line-height: 0.2rem;
    padding: 0.07rem 0 0.07rem 0.05rem;
    margin-bottom: 0.06rem;
    .border-radius();
    &.code {
      width: 50%;
      margin-right: 0.2rem;
    }
  }
  .code-img {
    height: 0.32rem;
  }
}

/* 会员中心 */

.mine-head {
  position: relative;
  width: 100%;
  text-align: center;
}
.mine-head-box {
  padding-top: 0.12rem;
  > img {
    border-radius: 100%;
    width: 0.9rem;
    height: 0.9rem;
    border: 1px solid #fff;
  }
}
.mine-head-name {
  color: #fff;
  font-size: 0.16rem;
  line-height: 1.5;
  text-shadow: 0 0.005rem 1px rgba(0, 0, 0, 0.3);
  margin-top: 0.02rem;
}
.scorenum-box {
  line-height: 1.2;
  font-size: 0.14rem;
  padding-bottom: 0.05rem;
}
.scorenum {
  font-size: 0.14rem;
  color: #fff;
  line-height: 1.2;
}

.mine-content {
  margin-bottom: 0.1rem;
  > ul > li {
    background: #fff;
    margin-top: 0.1rem;
    padding: 0 0.1rem;
  }
}
.mine-link {
  line-height: 0.36rem;
  font-size: 0.14rem;
  display: block;
  text-align: center;
  .t3;
  position: relative;
  &::before,
  &::after {
    top: 0;
    line-height: 0.36rem;
    font-family: 'iconfont';
    position: absolute;
    color: #9a75cd;
    font-size: 0.18rem;
  }
  &::before {
    left: 0;
  }
  &::after {
    right: 0;
    content: '\e617';
  }
}
.mine-link-signup {
  &::before {
    content: '\e619';
  }
}
.mine-link-score {
  &::before {
    content: '\e608';
  }
}
.mine-link-collect {
  &::before {
    content: '\e705';
  }
}
.mine-link-comment {
  &::before {
    content: '\e611';
  }
}
.mine-link-info {
  &::before {
    content: '\e67c';
  }
}
.mine-link-signupmg {
  &::before {
    content: '\e625';
  }
}
.mine-link-scoremg {
  &::before {
    content: '\e621';
  }
}
.mine-link-check {
  &::before {
    content: '\e613';
  }
}
.mine-link-commentmg {
  &::before {
    content: '\e729';
  }
}
.mine-link-endorse {
  &::before {
    content: '\e64d';
  }
}
.mine-link-card {
  &::before {
    content: '\e61f';
  }
}
.mine-link-coupon {
  &::before {
    content: '\e60d';
  }
}
.mine-link-ordered {
  &::before {
    content: '\e627';
  }
}

/* 搜索框 */
.search-bar {
  padding: 0.1rem 0.1rem 0;
  position: relative;
  background: #fff;
  display: flex;
  align-items: center;
  font-size: 0.12rem;
}
.search-bar-inner {
  display: flex;
  flex-wrap: nowrap;
  padding: 0 0.1rem;
  border: 1px solid #eee;
  border-radius: 0.16rem;
  overflow: hidden;
  background: #fff;
  width: 100%;
  height: 0.32rem;
  align-items: center;
  justify-content: space-between;
}
.search-bar-input {
  flex: auto;
  padding: 0.07rem 0;
  background-color: #fff;
  box-sizing: border-box;
  color: #666;
  font-size: 0.14rem;
}
.search-bar-span {
  font-size: 0.16rem;
  color: #999;
}
.search-select {
  background: #fff;
  flex: 1;
  padding: 0.1rem 0.1rem 0 0;
  position: relative;
  &::after {
    content: '';
    position: absolute;
    top: 0.22rem;
    right: 0.27rem;
    width: 0;
    height: 0;
    border-right: 4px solid transparent;
    border-left: 4px solid transparent;
    border-top: 6px solid #333;
  }
}
.search-select > select {
  width: 100%;
  height: 0.32rem;
  border-radius: 4px;
  border: 1px solid #eee;
  color: #666;
  font-size: 0.14rem;
  padding-left: 0.12rem;
  appearance: none;
  -webkit-appearance: none;
  -moz-appearance: none;
  -ms-appearance: none;
  -o-appearance: none;
  vertical-align: middle;
  outline: none;
  background: inherit;
}
.date-range {
  font-size: 0.12rem;
  color: #999;
}
.begintime,
.endtime {
  width: 0.61rem;
  font-size: 0.12rem;
  color: #999;
}

/* 订单详情 */
.textbox-wrap {
  margin-bottom: 0.1rem;
  background: #fff;
  padding: 0 0.1rem;
  overflow: hidden;
  > li {
    padding: 0.05rem 0;
    font-size: 0.14rem;
    line-height: 1.5;
    width: 100%;
    .head {
      font-weight: bold;
      font-size: 0.16rem;
      color: #333;
    }
    .tit {
      .t9;
    }
    .txt {
      .t3;
      .name {
        display: block;
      }
      > input {
        width: 100%;
        display: block;
        height: 0.28rem;
        font-size: 0.14rem;
        color: #333;
      }
    }
    .order-qrcode {
      width: 0.8rem;
      height: 0.8rem;
    }
    .explain {
      margin-left: 0.2rem;
      text-decoration: underline;
    }
  }
}

/* 2019.7.2分销 */
.fixed-pop {
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: 1px solid #eee;
  height: 40px;
  width: 100%;
  justify-content: center;
  align-items: center;
  background: #fff;
}
.fixed-pop .fixed-pop-inner {
  flex: 0 0 33%;
  border-radius: 4px;
  height: 36px;
  flex-direction: column;
  justify-content: center;
