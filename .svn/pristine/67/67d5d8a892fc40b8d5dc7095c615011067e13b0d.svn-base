.Admin_header .nav_main{width: 749px;}


.kf_pop{position:fixed; right:0px; top:200px; z-index: 11111; box-shadow:1px 1px 1px #d4d4d4; border-top: 2px solid #cf2c25;}
.kf_pop_main{background:#fff;  width:130px; padding: 0px 0px 20px 5px;  position:relative; overflow:hidden;}
.kf_pop_title{background:url(../img/pop_kf_dt.gif) no-repeat; width:32px; height:156px;   overflow:hidden; cursor:pointer; display:none;}
.kf_pop_close{background:url(../img/close.jpg) no-repeat; width:19px; height:19px; border:none; position:absolute; right:7px; top:8px; cursor:pointer;}
.kf_pop_list{margin:0 auto; width:120px;  overflow:hidden;}
.kf_pop_list li{margin-left: 10px; width:100px; height:28px; line-height:28px; margin-bottom:5px; *margin-bottom:0px; overflow:hidden; }
.kf_pop_list li a{display:block; color:#ba8100;}
.kf_pop_list .kf_pop_li{}
.kf_pop_list .kf_pop_li a{display:block; color:#964d00; width:100px; height:28px; line-height:28px;}
.kf_pop .cs_tel_ct{margin: 10px 0px 10px 0px; color: #CC0000; font-weight: bold; font-size: 16px;}
.kf_pop .tel{font-weight: bold; font-size: 14px; margin-top: 10px;}
.kf_pop .tel_number{font-weight: bold; font-size: 16px; color: #CC0000;}

.categorys_mini{position: relative; margin-top: 40px; cursor: pointer;   z-index: 1111111;}
.categorys_mini .cw_icon{color: #666; font-size: 12px; width: 63px;  height: 25px; border: 1px solid #ccc;  line-height: 25px; position: relative;  z-index: 1;  background: #fff;  padding: 0 5px;}
.categorys_mini .cw_icon .arrow-d{position: absolute; width: 12px; height: 7px; background: url(../img/site.png) -89px 1px no-repeat; right: 7px; top: 10px;  transition: .2s ease-in-out;  -webkit-transition: .2s ease-in-out;  -moz-transition: .2s ease-in-out;    -o-transition: .2s ease-in-out; overflow: hidden;}
.categorys_mini .current .arrow-d{transform:rotate(-180deg);-webkit-transform:rotate(-180deg);-moz-transform:rotate(-180deg);-o-transform:rotate(-180deg)}
.categorys_mini_main{display: none; position: absolute; top: 27px; left: 0px; width: 152px; padding: 13px; overflow: hidden; margin-top: -1px; background: #fff; border: 1px solid #ccc;}
.categorys_mini_main ul li{padding: 5px 0px;}
.categorys_mini_main ul li a{font-size: 12px; color: #666; padding: 3px 0px; line-height: 20px;}
.categorys_mini_main ul li a:hover{color: #CC0000}
.categorys_mini .current{border-bottom: 1px solid #fff;}

.Index_header .categorys_mini_main{width: 56px;}