/* 下拉框 */
.select-box {
  /* 下拉框 */
  padding: 0.1rem 0.1rem 0;
  font-size: 0.12rem;
  color: #666;
}
.select-box > form {
  width: 100%;
  justify-content: space-between;
}
.select-box > form .select {
  background: #eee;
  -webkit-border-radius: 0.15rem;
  -moz-border-radius: 0.15rem;
  -ms-border-radius: 0.15rem;
  -o-border-radius: 0.15rem;
  border-radius: 0.15rem;
  position: relative;
  height: 0.3rem;
  width: 0.95rem;
  padding-left: 0.12rem;
  color: #666;
  line-height: 0.3rem;
}
.select-box > form .select::after {
  font-family: 'iconfont' !important;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  content: '\e602';
  position: absolute;
  right: 0.03rem;
  top: 0.03rem;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%;
  border-radius: 50%;
  width: 0.23rem;
  height: 0.23rem;
  color: #fff;
  background: #ccc;
  text-align: center;
  line-height: 0.23rem;
  z-index: 0;
}
.select-box > form .select:nth-child(2)::after {
  content: '\e600';
}
.select-box > form .select:nth-child(3)::after {
  content: '\e616';
}
.select-box > form .select > select {
  font-family: 'Microsoft YaHei', sans-serif, Tahoma, Arial;
  height: 0.3rem;
  border: none;
  outline: none;
  appearance: none;
  -moz-appearance: none;
  -webkit-appearance: none;
  background: transparent;
  color: #666;
  position: relative;
  z-index: 1;
  padding-left: 0;
}
.select-box > form .select > select::-ms-expand {
  display: none;
}
.select-box > form .select.on::after {
  background: #ff8738;
}


