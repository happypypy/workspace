/* 价格红色显示 */
.red {
  color: red;
}
/* 积分扣除 */
.green {
  color: green;
}
/* 积分退款 */
.grayfont {
  color: #ccc;
}
/* 去掉浏览器滚动条 */
::-webkit-scrollbar {
  display: none;
}
/* 统一placehold样式 */
input::-webkit-input-placeholder {
  color: #aab2bd;
  font-size: 0.12rem;
}
/* 1px下边框 */
.border-line {
  position: relative;
  border: none;
}
.border-line::after {
  content: '';
  position: absolute;
  display: block;
  bottom: -1px;
  background: #eee;
  width: 100%;
  height: 1px;
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}
/* 自定义字体 */
@font-face {
  font-family: 'iconfont';
  src: url('../font/iconfont.eot');
  src: url('../font/iconfont.eot?#iefix') format('embedded-opentype'), url('../font/iconfont.woff2') format('woff2'),
    url('../font/iconfont.woff') format('woff'), url('../font/iconfont.ttf') format('truetype'),
    url('../font/iconfont.svg#iconfont') format('svg');
}
.iconfont {
  font-family: 'iconfont' !important;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.one-txt-cut {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
/* 按钮样式 */
.btns-right {
  padding-top: 0.02rem;
  width: 100%;
  text-align: right;
  overflow: hidden;
}
.btns-left {
  padding-top: 0.02rem;
  width: 100%;
  text-align: left;
  overflow: hidden;
}
.btns-center {
  padding: 0.1rem;
  width: 100%;
  text-align: center;
  overflow: hidden;
}
.normal-btn,
.del-btn,
.reply-btn,
.shield-btn {
  display: inline-block;
  padding: 0 0.1rem;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  font-size: 0.12rem;
  line-height: 0.28rem;
  color: #fff;
}
.several-btn1,
.several-btn2,
.several-btn3,
.several-btn4,
.several-btn5,
.several-btn6 {
  display: inline-block;
  padding: 0 0.05rem;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  font-size: 0.12rem;
  line-height: 0.28rem;
  color: #fff;
}
.pay-btn {
  display: inline-block;
  padding: 0 0.1rem;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  font-size: 0.12rem;
  line-height: 0.24rem;
  color: #fff;
  margin-left: 0.1rem;
}
.sub-btn,
.cancel-btn {
  display: inline-block;
  padding: 0 0.3rem;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  font-size: 0.14rem;
  line-height: 0.32rem;
  color: #fff;
}
.spread-btn {
  display: inline-block;
  height: 0.38rem;
  line-height: 0.38rem;
  padding: 0 0.1rem;
  font-weight: bold;
  color: #fff;
  white-space: nowrap;
  text-align: center;
  font-size: 0.14rem;
  border: none;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  width: 100%;
}
.scan-btn {
  display: inline-block;
  padding: 0 0.3rem;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  font-size: 0.14rem;
  line-height: 0.32rem;
  color: #666;
}
/* 清除浮动 */
.clearfix:after {
  display: block;
  clear: both;
  height: 0;
  content: '\20';
  visibility: hidden;
  overflow: hidden;
}
.clearfix {
  display: block;
  *zoom: 1;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.t3 {
  color: #333;
}
.t6 {
  color: #666;
}
.t9 {
  color: #999;
}
.flex {
  display: flex;
}
.flexCol {
  display: flex;
  flex-direction: column;
}
/* 设置页面底色 */
body {
  min-height: 100vh;
  margin: 0 auto;
  min-width: 320px;
  max-width: 640px;
  position: relative;
  background: #f4f4f4;
}
/* 撑开内容盒子 */
section.section {
  padding-top: 50px;
  flex: auto;
}
/* 下拉框 */
.select-bar {
  width: 100%;
  background: #fff;
  z-index: 11;
  position: relative;
}
.select-bar-tab {
  flex-wrap: nowrap;
  display: flex;
  justify-content: center;
  align-items: center;
}
.select-bar-tab > li {
  flex: 1;
  text-align: center;
  font-size: 0.14rem;
  color: #666;
  height: 0.36rem;
  line-height: 0.36rem;
  border-right: 1px solid #eee;
  border-bottom: 1px solid #eee;
  position: relative;
}
.select-bar-tab > li:last-child {
  border-right: none;
}
.select-bar-tab > li.fontColor .select-item {
  color: #666;
}
.select-bar-tab > li.on {
  background-color: #f4f4f4;
}
.select-bar-tab > li.active > span {
  position: relative;
  width: 6em;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: inline-block;
}
.select-bar-tab > li.active > span::after {
  content: '';
  width: 0;
  height: 0;
  display: inline-block;
  border-top: 0.08rem solid #b7b7b7;
  border-left: 0.04rem solid transparent;
  border-right: 0.04rem solid transparent;
  border-bottom: 0;
  margin-left: 0.04rem;
}
.select-bar-tab > li.fontColor > span:after {
  border-top: 0;
  border-left: 0.04rem solid transparent;
  border-right: 0.04rem solid transparent;
  border-bottom: 0.08rem solid #b7b7b7;
  vertical-align: 1px;
}
.select-wrapper {
  position: absolute;
  width: 100vw;
  height: 0;
  left: 0;
  top: 0.36rem;
}
.select-list {
  position: absolute;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background: #f4f4f4;
  display: none;
}
.select-item {
  width: 100%;
  max-height: 80vh;
  overflow-y: auto;
  position: absolute;
  top: 0;
  left: 0;
  overflow-y: scroll;
  z-index: 11;
  display: none;
}
.select-item li {
  padding-left: 0.28rem;
  line-height: 0.36rem;
  height: 0.36rem;
  color: #666;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  font-size: 0.14rem;
}
.select-list:nth-child(2) li {
  padding-left: calc(1.61rem);
}
.select-list:nth-child(3) li {
  padding-left: calc(2.86rem);
}
/* tab栏 */
.tabs {
  width: 100%;
  border-top: 1px solid #ccc;
  border-bottom: 1px solid #ccc;
  background: #eee;
  text-align: center;
}
.tabs > li {
  flex: 1;
  text-align: center;
  line-height: 0.4rem;
  color: #333;
}
.tabs > li:first-child {
  border-right: 1px solid #ccc;
}
.tabs > li.active {
  background: #fff;
}
.tabs > li > div:first-child {
  font-size: 0.22rem;
  line-height: 0.24rem;
}
.tabs > li > div:nth-child(2) {
  line-height: 0.14rem;
  font-size: 0.12rem;
}
.tab-content {
  padding: 0.1rem;
  font-size: 0.14rem;
  color: #666;
  background: #fff;
}
.tab-content .content {
  display: none;
}
.tab-content .content.selected {
  display: block;
}
/* 加载 */
@keyframes myload {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(359deg);
  }
}
.load {
  display: none;
}
.load .iconload {
  line-height: 1;
  font-size: 0.12rem;
  animation: myload 0.5s linear infinite;
  display: inline-block;
  color: #999;
  position: relative;
  left: 50%;
  margin-left: -0.5em;
}
/* 弹出层 */
body .my-layer .layui-layer-content {
  font-size: 0.14rem;
  line-height: 1.5;
  padding: 0.1rem 0.2rem 0.1rem;
  color: #333;
}
body .my-layer .layui-layer-content .order-info-ul li {
  /* height: 0.36rem; */
  border-bottom: 1px solid #eee;
  line-height: 0.36rem;
}
/* 文字说明 */
.indent-ul {
  padding-left: 0.14rem;
}
.indent-ul li {
  list-style: decimal;
}
body .my-layer .layui-layer-btn {
  padding: 0.1rem;
}
body .my-layer .layui-layer-btn a {
  border: 0;
  font-size: 0.14rem;
  line-height: 0.28rem;
  height: 0.28rem;
  padding: 0 0.3rem;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  color: #fff;
}
/* 头部 */
header {
  height: 50px;
  background: rgba(255, 255, 255, 0.95);
  padding: 0 10px;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 88;
  border-bottom: 1px solid #eee;
  align-items: center;
  justify-content: space-between;
}
.logo {
  align-items: center;
  height: 40px;
}
.logo > img {
  height: 30px;
}
/* 右边按钮菜单栏 */
.menu-btns {
  height: 25px;
  font-size: 14px;
  text-align: center;
  line-height: 25px;
}
.menu-btn3,
.menu-btn1,
.menu-btn2 {
  width: 30px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
}
.menu-btn1 {
  background: #ccc;
  margin-right: 6px;
  color: #666;
}
.menu-btn2,
.menu-btn3 {
  color: #fff;
}
.menu-btn3 {
  display: none;
}
/* 侧边列表 */
aside .menu {
  position: fixed;
  top: 0;
  right: -70%;
  width: 70%;
  height: 100%;
  padding-top: 0.3rem;
  background: #373a41;
  z-index: 99;
}
aside .menu li > a {
  font-size: 0.14rem;
  color: #fff;
  line-height: 0.4rem;
  padding-left: 0.3rem;
  display: block;
}
/* 侧边动画 */
header,
section,
footer,
.menu {
  -webkit-transition: transform 0.3s;
  -o-transition: transform 0.3s;
  transition: transform 0.3s;
}
.menu-cover {
  opacity: 0;
  background: #fff;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 98;
  position: fixed;
  display: none;
}
body.open-menu .menu {
  -webkit-transform: translateX(-100%);
  -ms-transform: translateX(-100%);
  -o-transform: translateX(-100%);
  transform: translateX(-100%);
}
body.open-menu header,
body.open-menu section,
body.open-menu footer {
  -webkit-transform: translateX(-70%);
  -ms-transform: translateX(-70%);
  -o-transform: translateX(-70%);
  transform: translateX(-70%);
}
/* footer */
.footer {
  width: 100%;
  position: relative;
  z-index: 6;
}
.copyright-wrap {
  text-align: center;
  color: #999;
  padding: 0.1rem 0.1rem 0.05rem;
  font-size: 0.12rem;
}
.copyright {
  font-size: 0.14rem;
  margin-bottom: 0.06rem;
}
.footernav-wrap {
  padding-bottom: 51px;
}
.footernav-wrap-inner {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 8;
  width: 100%;
  background: #fff;
  border-top: 1px solid #eee;
}
.footernavbar {
  justify-content: center;
  align-items: center;
  flex-wrap: nowrap;
  height: 50px;
}
.footernavbar > li {
  flex: 1;
  text-align: center;
  padding: 1px;
}
.footernavbar-link {
  color: #666;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
}
.footernavbar-link.on {
  color: #fff;
}
.footernavbar-link.on > p {
  font-weight: bold;
}
.footernavbar-link > span > i {
  font-size: 22px;
  font-family: 'iconfont' !important;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-style: normal;
}
.footernavbar-link > p {
  font-size: 12px;
  font-weight: bold;
  line-height: 1.5;
}
.footer-comment-form {
  position: absolute;
  bottom: 51px;
  z-index: 10;
  width: 100%;
  background: #e7e7e7;
  padding: 10px;
  box-sizing: border-box;
  display: none;
}
.footer-comment-textarea > textarea {
  display: block;
  background-color: #f7f7f7;
  resize: none;
  width: 100%;
  height: 70px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  border: 1px solid #eee;
  padding: 6px 4px;
  color: #333;
  box-sizing: border-box;
  font-size: 14px;
}
.footer-comment-textarea > p {
  color: #333;
  font-size: 14px;
}
.footer-comment-file {
  padding-top: 5px;
}
.footer-comment-file > p {
  color: #333;
  font-size: 14px;
  margin-bottom: 2px;
}
.file-label {
  position: relative;
}
.file-label > input[type='file'] {
  position: absolute;
  right: 0;
  top: 0;
  opacity: 0;
}
.file-label > input[type='button'] {
  margin-right: 2px;
  background: #fff;
  border: 1px solid #ccc;
  font-size: 12px;
  line-height: 1.5;
  padding: 0 2px;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  color: #666;
}
.file-label > span {
  font-size: 12px;
}
.comment-submit {
  margin-top: 10px;
  text-align: center;
}
.comment-submit > input {
  color: #666;
  background: #fff;
  -webkit-border-radius: 4px;
  -moz-border-radius: 4px;
  -ms-border-radius: 4px;
  -o-border-radius: 4px;
  border-radius: 4px;
  font-size: 14px;
  padding: 0 20px;
  line-height: 2;
}
.comment-submit > input:first-child {
  margin-right: 6px;
}
/* 首页 */
/* 轮播 */
.banner {
  margin-bottom: 0.1rem;
  background: #fff;
}
.swiper-container1 {
  margin: 0 auto;
  position: relative;
  overflow: hidden;
  list-style: none;
  padding: 0;
  z-index: 1;
}
.swiper-container1 .swiper-wrapper {
  position: relative;
  width: 100%;
  height: 100%;
  z-index: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-transition-property: -webkit-transform;
  transition-property: -webkit-transform;
  -o-transition-property: transform;
  transition-property: transform;
  transition-property: transform, -webkit-transform;
  -webkit-box-sizing: content-box;
  box-sizing: content-box;
}
.swiper-container1 .swiper-slide {
  -webkit-flex-shrink: 0;
  -ms-flex-negative: 0;
  flex-shrink: 0;
  width: 100%;
  height: calc(2.5rem);
  position: relative;
  -webkit-transition-property: -webkit-transform;
  transition-property: -webkit-transform;
  -o-transition-property: transform;
  transition-property: transform;
  transition-property: transform, -webkit-transform;
}
.swiper-container1 .swiper-slide img {
  width: 100%;
  height: 100%;
}
.swiper-container1 .swiper-pagination {
  position: absolute;
  text-align: center;
  -webkit-transition: 0.3s opacity;
  -o-transition: 0.3s opacity;
  transition: 0.3s opacity;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  z-index: 10;
}
.swiper-container1 .swiper-pagination-bullets,
.swiper-container1 .swiper-pagination-custom,
.swiper-container1 .swiper-pagination-fraction {
  bottom: 0;
  left: 0;
  width: 100%;
}
.swiper-container1 .swiper-pagination-bullet {
  width: 0.06rem;
  height: 0.06rem;
  display: inline-block;
  border-radius: 100%;
  background: #000;
  opacity: 0.2;
}
.swiper-container1 .swiper-pagination-bullet-active {
  opacity: 1;
  background: #fff;
}
.swiper-container1 .swiper-pagination-bullet {
  margin: 0 0.02rem;
}
.swiper-container1 .swiper-notification {
  position: absolute;
  left: 0;
  top: 0;
  pointer-events: none;
  opacity: 0;
  z-index: -1000;
}
.swiper-container1 .swiper-container-fade .swiper-slide-active {
  pointer-events: auto;
}
.swiper-container1 .swiper-container-fade .swiper-slide {
  pointer-events: none;
  -webkit-transition-property: opacity;
  -o-transition-property: opacity;
  transition-property: opacity;
}
.nav {
  /* 导航栏 */
  padding: 0 0.1rem;
