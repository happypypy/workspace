{volist name="$subscribe_record_list" id="vo"}
<li class="list-item  flex">
  <div class="list-item-txt flexCol">
    <div class="info flex">
      <div class="tit tit-w5">预约编号</div>：
      <div class="txt">{$vo.subscribe_no}</div>
    </div>
    <div class="info flex">
        <div class="tit tit-w5">会员卡名称</div>：
        <div class="txt fontColor">{$vo.member_cart_name}</div>
    </div>
    <div class="info flex">
      <div class="tit tit-w5">项目名称</div>：
      <div class="txt">{$vo.subscribe_object_name}</div>
    </div>
    <div class="info flex">
      <div class="tit tit-w5">预约次数</div>：
      <div class="txt">{$vo.subscribe_number}</div>
    </div>
    <div class="info flex">
        <div class="tit tit-w5">创建时间</div>：
        <div class="txt">{$vo.create_time|date="Y-m-d H:i:s",###}</div>
    </div>
    <div class="info flex">
        <div class="tit tit-w5">是否签到</div>：
        <div class="txt">{$vo.is_signin ? '已签到' : '未签到'}</div>
    </div>
    <div class="info flex">
        <div class="tit tit-w5">签到时间</div>：
        <div class="txt">{notempty name="$vo.signin_time"}{$vo.signin_time|date="Y-m-d H:i:s",###}{/notempty}</div>
    </div>
    <div class="btns-right">
      <input class="several-btn1" type="button" value="订单详情" onclick="location.href='/{$sitecode}/subscriberecorddetail/{$vo['id']}'">
      {if condition="$vo.subscribe_status==0"}
      <input class="several-btn2" type="button" value="取消订单" onclick="location.href='javascript:cancel_order({$vo['id']});'">
      {else/}
      <input class="several-btn2" type="button" value="已取消" onclick="javascript:;">
      {/if}
    </div>
  </div>
</li>
{/volist}