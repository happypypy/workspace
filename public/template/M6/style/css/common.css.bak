@charset "utf-8";

/* CSS Document */
html{
    -ms-text-size-adjust:100%;
    -webkit-text-size-adjust:100%;
    font-size:100px;
}
body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,img,ins,kbd,q,s,samp,small,strike,sub,sup,tt,var,b,u,i,center,dl,dt,dd,ol,ul,li,fieldset,form,label,article,aside,canvas,details,embed,figure,figcaption,footer,header,hgroup,menu,nav,output,ruby,section,summary,time,mark,audio,video{
    padding:0;
    margin:0;
    border:0;
    vertical-align:baseline;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
h1,h2,h3,h4,h5,h6{
    font-weight:normal;
}
body{
    min-width: 320px;
    font-size: 16px;
    text-rendering: optimizeLegibility;
    font-family: -apple-system-font,"Helvetica Neue","PingFang SC","Hiragino Sans GB","Microsoft YaHei",sans-serif;
}
table{
    margin:auto;
    text-align:left;
    border-collapse:collapse;
    border-spacing:0;
}
ol,ul{
    list-style:none;
    list-style-type:none;
}
a img,img{
    vertical-align:middle;
    border:none;
}

/* a style */
a{
    text-decoration: none;
    -webkit-tap-highlight-color: rgba(0,0,0,0); 
}
a:visited{
    text-decoration: none;
    outline: none;
}
a:hover{
    text-decoration: none;
    outline: none;
}
a:active{
    outline: none;
}
a:focus{
    outline: none;
}

/* form style */
input,textarea,select,button{
    vertical-align: middle;
    outline: none;
    background: inherit;
    appearance: none;
    -webkit-appearance: none;  
    -moz-appearance: none;
    -ms-appearance: none;
    -o-appearance: none;
    padding: 0;
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}
label{
    -webkit-tap-highlight-color: rgba(0,0,0,0);
}

/* clearfix style */
.clear{
    clear:both;
    height:0;
    visibility:hidden;
    overflow:hidden;
}
.clearfix:after{
    display: block;
    clear: both;
    height: 0;
    content: "\20";
    visibility: hidden;
    overflow: hidden;
}
.clearfix{
    display: block;
    *zoom: 1;
}
.ovh{
    overflow: hidden;
}
*html .clearfix{
    height: 1%;
}
.flex{
    display: -webkit-flex;
    display: -webkit-box;
    display: -moz-flex;
    display: -ms-flex;
    display: -o-flex;
    display: flex;
}
.flex-middle{
    -webkit-box-pack: center;
    -moz-box-pack: center;
    -ms-flex-align:center;/* IE 10 */
    -webkit-align-items: center;
    -moz-align-items: center;
    align-items: center;
}
.flex-center{
    -webkit-box-align: center;
    -moz-box-align: center;
    -ms-flex-pack:center;  
    -webkit-justify-content: center;
    -moz-justify-content: center;
    justify-content: center;
}
.fx1{
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    -moz-flex: 1;
    -ms-flex: 1;
    -o-flex: 1;
    flex: 1;
}
.space-between{
    -webkit-justify-content: space-between;
    justify-content: space-between;
}




/*page1*/
.site-section{
	position:relative;
	min-width: 320px;
	max-width: 640px;
	margin: 0 auto;
	font-size: 0.16rem;
	padding-top: 0.5rem;
}
.index-bg{
	background: url("../images/bg_03.jpg") no-repeat center top;
	background-size: 3.2rem 9.3rem;
	background-attachment: fixed;
}
header{
	height: 0.5rem;
	background: rgba(255,255,255,.95);
	padding: 0 0.1rem;
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	z-index: 9;
	box-shadow: 0 0 6px rgba(0,0,0,.2);
}
header .logo{
	height: 0.32rem;
	margin-top: 0.09rem;
	display: block;
	float: left;
}
header .logo img{
	height: 0.32rem;
	width: auto;
}
header .btns{
	float: right;
	margin-top: 0.135rem;
}
header .btns a{
	display: block;
	float: left;
	width: 0.265rem;
	height: 0.215rem;
	background-size: 0.265rem 0.215rem;
	background-repeat: no-repeat;
	background-position: 0;
}
header .btns .btn1{
	background-image: url("../images/header_btn1_03.png");
	margin-right: 0.05rem;
}
header .btns .btn2{
	background-image: url("../images/header_btn2_03.png");
}
header .btns .btn3{
	background-image: url("../images/btn_03_03.png");
	display: none;
}

.banner{
	height: 1.79rem;
	padding: 0 0.09rem;
	position: relative;
}
.banner .swiper-container{
	border-radius: 0.03rem;
	box-shadow: 0 0.01rem 0.05rem rgba(0,0,0,.24);
}
.banner img{
	width: 100%;
	height: 1.5rem;
	border-radius: 0.03rem;
}
.banner .swiper-pagination{
	bottom: 0.12rem;
	width: 100%;
	left: 0;
	z-index:  2;
}
.banner .swiper-pagination-bullet{
	width: 0.06rem;
	height: 0.06rem;
	background: #fff;
	opacity: .8;
	margin: 0 0.025rem;
}
.banner .swiper-pagination-bullet-active{
	background: #e50013;
	opacity: 1;
}

.nav{
	height: 1.77rem;
	padding-top: 0.19rem;
}
.nav li{
	width: 0.6rem;
	height: 0.73rem;
	margin: 0 0.1rem;
	overflow: hidden;
	text-align: center;
	float: left;
}
.nav li img{
	height: 0.4rem;
}
.nav li p{
	font-size: 0.1rem;
	line-height: 0.14rem;
	color: #666;
	padding-top: 0.04rem;
}

.bar{
	height: 0.75rem;
}
.bar a{
	display: block;
	width: 1.45rem;
	height: 0.75rem;
	float: left;
	margin-left: 0.1rem;
}
.bar img{
	width: 1.45rem;
	height: 0.75rem;
	display: block;
	border-radius: 0.03rem;
}

.list{
	padding: 0.21rem 0 0.1rem;
	overflow: hidden;
}
.list li{
	margin-left: 0.1rem;
	margin-bottom: 0.1rem;
	width: 1.45rem;
	height: 1.6rem;
	float: left;
	border-radius: 0.03rem;
	box-shadow: 0 0.01rem 0.05rem rgba(0,0,0,.24);
}
.list img{
	width: 1.45rem;
	height: 0.98rem;
	border-top-left-radius: 0.03rem;
	border-top-right-radius: 0.03rem;
}
.list .word{
	height: 0.62rem;
	border-bottom-left-radius: 0.03rem;
	border-bottom-right-radius: 0.03rem;
	background: #fff;
	position: relative;
	padding: 0 0.07rem;
}
.list .word .tit{
	font-size: 0.1rem;
	color: #333;
	line-height: 0.14rem;
	padding: 0.05rem 0 0.03rem;
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis;
}
.list .word .txt{
	font-size: 0.09rem;
	color: #999;
	line-height: 0.14rem;
	height: 0.28rem;
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.list .word .info{
	font-size: 0.09rem;
	color: #fff;
	line-height: 0.19rem;
	height: 0.19rem;
	position: absolute;
	width: 100%;
	left: 0;
	top: -0.19rem;
	background: rgba(0,0,0,.5);
}
.list .word .view{
	float: left;
	padding-left: 0.07rem;
}
.list .word .time{
	float: right;
	padding-right: 0.07rem;
}


footer {padding-top: 70px;}
.footer{
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	background: rgba(255,255,255,1);
	-moz-box-shadow:0px 0px 5px rgba(0,0,0,.2);
	-webkit-box-shadow:0px 0px 5px rgba(0,0,0,.2);
	box-shadow:0px 0px 5px rgba(0,0,0,.2);
	z-index: 8;
}
.footer ul{
	display: -webkit-box;
	display: -ms-flexbox;
	display: box;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
}
.footer ul li{
	-webkit-box-flex: 1;
	text-align: center;
	padding: 4px 0;
}
.footer ul li a{
	display: inline-block;
	padding: 0 0 4px;
	width: 70%;
	color: #777777;
	border-radius: 4px;
}
.footer ul li.on a{
	background: #ff7902;
	color: #fff;
}
.footer ul li.on a p{
	font-weight: bold;
}
.footer ul li a span{
	display: block;
	height: 28px;
	width: 28px;
	margin-left: auto;
	margin-right: auto;
}
.footer ul li a span i{
	display: inline-block;
}
.footer ul li a p{
	font-size: 14px;
	line-height: 16px;
}
.footer ul li a i.earth{
	font-size: 24px;
}
.footer ul li a i.coffee{
	font-size: 20px;
	padding: 3px 0 0 2px;
}
.footer ul li a i.head{
	font-size: 21px;
}
.footer ul li a i.message{
	font-size: 22px;
}



.menu{
	position: fixed;
	top: 0;
	right: 0;
	width: 78%;
	height: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	padding-top: 0.3rem;
	background: #373a41;
	z-index: 99;
	-webkit-transform: translateX(100%);
	-ms-transform: translateX(100%);
	-o-transform: translateX(100%);
	transform: translateX(100%);
}
header,
.site-section,
.menu{
	-webkit-transition: transform 0.3s;
	-o-transition: transform 0.3s;
	transition: transform 0.3s;
}
.menu-cover{
	opacity: 0;
	background: #fff;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 98;
	position: fixed;
	display: none;
}
body.open-menu .menu{
	-webkit-transform: translateX(0);
	-ms-transform: translateX(0);
	-o-transform: translateX(0);
	transform: translateX(0);
}
body.open-menu header,
body.open-menu .site-section{
	-webkit-transform: translateX(-78%);
	-ms-transform: translateX(-78%);
	-o-transform: translateX(-78%);
	transform: translateX(-78%);
}
body.open-menu header .btns .btn2{
	display: none;
}
body.open-menu header .btns .btn3{
	display: block;
}
body.open-menu .menu-cover{
	display: block;
}

.menu ul li a{
	font-size: 0.13rem;
	color: #fff;
	line-height: 0.42rem;
	display: block;
	padding-left: 0.35rem;
}
.menu ul li{
	position: relative;
}
.menu ul li:after{
	background: rgba(255,255,255,.2);
}







.rich_media{
	font-size: 16px;
    word-wrap: break-word;
    -webkit-hyphens: auto;
    -ms-hyphens: auto;
    hyphens: auto;
}
.rich_media_area_primary{
	position: relative;
    padding: 20px 15px 15px;
    background-color: #fff;
}
.rich_media_title{
	margin-bottom: 10px;
    line-height: 1.4;
    font-weight: 400;
    font-size: 24px;
}
.rich_media_meta{
	display: inline-block;
    vertical-align: middle;
    margin-right: 8px;
    margin-bottom: 10px;
    font-size: 16px;
}
.rich_media_meta_list {
	position:relative;
	z-index:1;
	margin-bottom: 18px;
    line-height: 20px;
    font-size: 14px;
		color: #666;
}
.rich_media_content {
	position:relative
}
.profile_container {
	width:535px;
	position:absolute;
	top:100%;
	left:65px;
	font-size:14px;
	*margin-top:10px
}
.profile_inner {
	position:relative;
	padding:30px 22px 36px 144px;
	background-color:#fff;
	border:1px solid #d9dadc;
	*zoom:1
}
.profile_arrow_wrp {
	position:absolute;
	left:22px;
	top:-8px
}
.profile_arrow {
	display:inline-block;
	width:0;
	height:0;
	border-width:8px;
	border-style:dashed;
	border-color:transparent;
	border-top-width:0;
	border-bottom-color:#d9dadc;
	border-bottom-style:solid;
	position:absolute;
	top:0
}
.profile_arrow.arrow_in {
	margin-top:1px;
	border-bottom-color:#fff
}
.profile_avatar {
	position:absolute;
	width:100px;
	left:24px;
	top:24px;
	height:100px!important
}
.profile_nickname {
	font-size:16px;
	font-weight:400
}
.profile_meta {
	margin-top:5px;
	overflow:hidden;
	*zoom:1
}
.profile_meta_label {
	float:left;
	width:4em;
	margin-right:1em
}
.profile_meta_value {
	display:block;
	overflow:hidden;
	*zoom:1;
	color:#adadad
}
.icon_verify {
	width:16px;
	height:16px;
	vertical-align:middle;
	display:inline-block;
	line-height:9em;
	overflow:hidden
}
.icon_verify.success {
	background:transparent url(//res.wx.qq.com/mmbizwap/en_US/htmledition/images/icon/wxverify/icon_verify_success238f08.png) no-repeat 0 0
}
span.rich_media_meta_nickname {
	display:inline-block
}
a.rich_media_meta_nickname {
	display:none
}
.rich_media_inner {
	position:relative
}
.qr_code_pc_outer {
	display:none!important;
	position:fixed;
	left:0;
	right:0;
	top:20px;
	color:#717375;
	text-align:center
}
.qr_code_pc_inner {
	position:relative;
	width:740px;
	margin-left:auto;
	margin-right:auto
}
.qr_code_pc {
	position:absolute;
	right:-140px;
	top:0;
	width:140px;
	padding:16px;
	border:1px solid #d9dadc;
	background-color:#fff;
	word-wrap:break-word;
	word-break:break-all
}
.qr_code_pc p {
	font-size:14px;
	line-height:20px
}
.qr_code_pc_img {
	width:102px;
	height:102px
}
.rich_media_meta_text{
	color: #8c8c8c;
}
.rich_media_meta_list em {
    font-style: normal;
}
a.rich_media_meta_nickname {
    display: none;
}
@media screen and (min-width:1025px) {
	.qr_code_pc_outer {
		display:block!important
	}
}

.meta_original_tag {
    display: inline-block;
    vertical-align: middle;
    padding: 1px .5em;
    border: 1px solid #9e9e9e;
    color: #8c8c8c;
    border-top-left-radius: 20% 50%;
    -moz-border-radius-topleft: 20% 50%;
    -webkit-border-top-left-radius: 20% 50%;
    border-top-right-radius: 20% 50%;
    -moz-border-radius-topright: 20% 50%;
    -webkit-border-top-right-radius: 20% 50%;
    border-bottom-left-radius: 20% 50%;
    -moz-border-radius-bottomleft: 20% 50%;
    -webkit-border-bottom-left-radius: 20% 50%;
    border-bottom-right-radius: 20% 50%;
    -moz-border-radius-bottomright: 20% 50%;
    -webkit-border-bottom-right-radius: 20% 50%;
    font-size: 15px;
    line-height: 1.1;
}
@media (-webkit-min-device-pixel-ratio: 2) and (max-device-width: 667px) and (min-device-width: 375px)
.rich_media_inner,
.rich_media_meta,
.discuss_list,
.rich_media_extra,
.title_tips .tips {
	font-size: 17px;
}





